{
  "name": "My Bookmarks",
  "version": "1.1",
  "description": "Chrome Plugin",
  "permissions": [
    "bookmarks"
  ],
  "browser_action": {
      "default_title": "Chrome Snips",
      "default_icon": "icon.png",
      "default_popup": "index.html"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
